;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Aphid Festival Resort"
config/description="Aphid Festival Demo"
config/version="0.1.0.0"
run/main_scene="res://scenes/menu.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://sprites/ui/leaf_cover.png"

[autoload]

GameManager="*res://scripts/managers/GameManager.cs"
OptionsManager="*res://scripts/managers/OptionsManager.cs"
SoundManager="*res://scripts/managers/SoundManager.cs"

[display]

window/size/mode=2
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="Aphid Festival"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://databases/": "blue",
"res://items/": "orange",
"res://scenes/": "gray",
"res://scripts/": "red",
"res://sfx/": "pink",
"res://skins/": "teal",
"res://sprites/": "purple"
}

[filesystem]

import/blender/enabled=false

